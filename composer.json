{
  "name": "dscmall/laravel-scout-elastic",
  "description": "Elastic Driver for Laravel Scout",
  "license": "MIT",
  "keywords": [
    "laravel",
    "scout",
    "elasticsearch",
    "elastic"
  ],
  "require": {
    "php": "^7.2",
    "elasticsearch/elasticsearch": "^7.11",
    "laravel/scout": "^8.6"
  },
  "suggest": {
    "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^7.11.0)."
  },
  "autoload": {
    "psr-4": {
      "Dscmall\\Scout\\Elastic\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dscmall\\Scout\\Elastic\\LaravelScoutElasticProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
